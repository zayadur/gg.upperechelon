    "homepage": "http://www.steven-clark.online/phpBB3-Extensions",
    "version": "0.5.6-dev",
    "time": "2017-06-06",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Steven Clark",
            "email": "info@steven-clark.online",
            "homepage": "http://www.steven-clark.online/",
            "role": "Author/Developer"
        }
    ],
    "require": {
        "php": ">= 5.4.0"
    },
    "extra": {
        "display-name": "Topic/Post Reactions",
        "soft-require": {
            "phpbb/phpbb": " >= 3.2.0"
        },
		"version-check": {
			"host": "www.steven-clark.online",
			"directory": "/phpBB3-Extensions/version",
			"filename": "reaction_version.json"
		}		
    }
