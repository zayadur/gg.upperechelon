<IfModule mod_rewrite.c>
Options -MultiViews
RewriteEngine On
RewriteBase /ips4/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule \.(js|css|jpeg|jpg|gif|png|ico|map)(\?|$) /ips4/404error.php [L,NC]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /ips4/index.php [L]
</IfModule>